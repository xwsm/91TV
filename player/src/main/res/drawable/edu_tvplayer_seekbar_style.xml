<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/x18"/>
            <solid android:color="@color/edu_tvplayer_seekbar_bg_color"/>
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
        <shape>
            <corners android:radius="@dimen/x18"/>
            <solid android:color="@color/edu_tvplayer_seekbar_secondaryProgress_color"/>
        </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
        <shape>
            <corners android:radius="@dimen/x18"/>
            <solid android:color="@color/edu_tvplayer_seekbar_progress_color"/>
        </shape>
        </clip>
    </item>

</layer-list>